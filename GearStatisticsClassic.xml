<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="GearStatisticsClassic.lua"/>
	<Script file="GSC_CharFrame.lua"/>

	<Frame name="GS_Frame" toplevel="true" movable="true" hidden="true" enableMouse="true" parent="UIParent" enableKeyboard="false">
		<Scripts>
			<OnLoad>
 				GS_OnLoad(self);
			</OnLoad>
			<OnEvent>
				GS_OnEvent(self, event, ...);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>